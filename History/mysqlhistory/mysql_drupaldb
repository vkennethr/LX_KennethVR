create database drupaldb;
create user sysadmin@#HOSTNAME;
create database drupaldb;
create database drupaldb1;
CREATE USER drupaldb1@localhost IDENTIFIED BY 'lolo';
GRANT SELECT,INSERT,UPDATE,DELETE,CREATE,DROP,INDEX,ALTER,CREATE TEMPORARY TABLES,LOCK TABLES ON drupal.* TO drupaluser@localhost;
GRANT SELECT,INSERT,UPDATE,DELETE,CREATE,DROP,INDEX,ALTER,CREATE TEMPORARY TABLES,LOCK TABLES ON drupaldb1.* TO drupaldb1@localhost;
flush privileges;
show databases;
history;
history drupaldb1;
information_schema;
